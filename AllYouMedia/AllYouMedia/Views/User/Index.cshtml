                xkey: 'm',
                parseTime: false,
                ykeys: ['Sales', 'Fans'],
                label:"Months",
                labels: ['Sales', 'Fans'],
                lineColors: ['#3c8dbc', '#a0d0e0'],
                hideHover: 'auto'
            });

            area.options.labels.forEach(function (label, i) {
                var legendItem = $('<span></span>').text(label).prepend('<br><span>&nbsp;</span>');
