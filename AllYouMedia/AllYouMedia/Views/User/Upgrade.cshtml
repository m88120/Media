<script type="text/javascript">
    $(document).ready(function () {
        $("#Country").change(function () {
            $('.updateSpinner').show();
            $("#State").empty();
            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetStates", "User")',
                dataType: 'json',
                data: { uid: $("#Country").val() },
                success: function (states) {
                 $('.updateSpinner').hide();
                    $.each(states, function (i, state) {
                        $("#State").append('<option value="' + state.Value + '">' +
                            state.Text + '</option>');
                    });
                }, error: function (ex) { $("#State").append('<option value="' + "" + '">' + "<< Select State>>" + '</option>'); $('.updateSpinner').hide(); }
            }); return false;
        })
    });
</script>
